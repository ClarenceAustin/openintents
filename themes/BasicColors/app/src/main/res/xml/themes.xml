<themes xmlns:oi="http://schemas.openintents.org/android/themes" >
	<!-- Themes for OI Shopping List -->
	<attributeset oi:name="org.openintents.shoppinglist" >
		<theme oi:title="@string/basic_colors_red"
			oi:style="@style/Theme.BasicColors.Red" />
		<theme oi:title="@string/basic_colors_green"
			oi:style="@style/Theme.BasicColors.Green" />
		<theme oi:title="@string/basic_colors_blue"
			oi:style="@style/Theme.BasicColors.Blue" />
	</attributeset>
	
	<!-- Themes for OI Notepad. -->
	<attributeset oi:name="org.openintents.notepad" >
		<theme oi:title="@string/basic_colors_red"
			oi:style="@style/Theme.BasicColors.Red.Notepad" />
		<theme oi:title="@string/basic_colors_green"
			oi:style="@style/Theme.BasicColors.Green.Notepad" />
		<theme oi:title="@string/basic_colors_blue"
			oi:style="@style/Theme.BasicColors.Blue.Notepad" />
	</attributeset>
</themes>